<apex:page controller="ChooseQuiz">
    <apex:form >
        <apex:pageBlock title="New Attempt">
            <apex:pageBlockSection >
                <apex:outputText value="{!player.Name}"/>
                <apex:inputField value="{!NewAttempt.Quiz__c}"/>
            </apex:pageBlockSection>
            <table style="width: 100%">
            <tr>
                <td align="center">
                    <apex:commandButton action="{!SaveAttempt}" value="Start Quiz"/>
                </td>
            </tr>
            </table>
        </apex:pageBlock>
    </apex:form>
</apex:page>

